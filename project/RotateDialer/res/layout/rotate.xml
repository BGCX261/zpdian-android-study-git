<?xml version="1.0" encoding="utf-8"?>
<com.gangle.RotateLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/subwindow_container"
    android:background="#ff000000"
    android:layout_width="match_parent"
    android:gravity="center"
    android:layout_height="match_parent">
    <ImageView
        android:id="@+id/bg"
        android:src="@+drawable/yy"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/middle"
        android:gravity="center"
        android:layout_marginLeft="108dp"
        android:layout_marginTop="37dp"
        android:layout_width="365dp"
        android:layout_height="wrap_content">
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/button_phone"
                android:layout_width="122dip"
                android:src="@drawable/button_phone"
                android:background="#00000000"
                android:layout_height="76dip" />
            <ImageButton
                android:id="@+id/button_navi"
                android:layout_width="122dip"
                android:src="@drawable/button_navi"
                android:layout_height="76dip"
                android:background="#00000000" />
            <ImageButton
                android:id="@+id/button_music"
                android:layout_width="122dip"
                android:src="@drawable/button_music"
                android:layout_height="76dip"
                android:background="#00000000" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:gravity="center"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/button_dial"
                android:layout_width="91dp"
                android:layout_height="56dp"
                android:src="@drawable/button_dial"
                android:background="#00000000" />
            <ImageButton
                android:id="@+id/button_dial_record"
                android:layout_width="90dp"
                android:layout_height="56dp"
                android:src="@drawable/button_dial_record"
                android:background="#00000000" />
            <ImageButton
                android:id="@+id/button_contacts"
                android:layout_width="90dp"
                android:layout_height="56dp"
                android:src="@drawable/button_contacts"
                android:background="#00000000" />
            <ImageButton
                android:id="@+id/button_fav"
                android:layout_width="91dp"
                android:layout_height="60dp"
                android:src="@drawable/button_fav"
                android:background="#00000000" />
        </LinearLayout>
        <LinearLayout
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">
            <ImageButton
                android:id="@+id/button_keypad"
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:src="@drawable/button_keypad"
                android:background="#00000000" />
            <TextView
                android:id="@+id/text"
                android:layout_width="244dp"
                android:layout_height="50dp"
                android:gravity="center_vertical"
                android:textColor="#FFFFFFFF"
                android:textSize="28sp"
                android:cursorVisible="false"
                android:singleLine="true"
                android:background="#00000000" />
            <ImageButton
                android:id="@+id/button_delete"
                android:layout_width="60dp"
                android:layout_height="50dp"
                android:src="@drawable/button_delete"
                android:background="#00000000" />
        </LinearLayout>
        <LinearLayout
            android:id="@+id/region_dial_pad"
            android:orientation="horizontal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="#00000000">
            <LinearLayout
                android:id="@+id/region_numbers"
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <LinearLayout
                    android:id="@+id/region_one_to_three"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/button_one"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/one"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_two"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/two"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_three"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/three"
                        android:background="#00000000" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/region_four_to_six"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/button_four"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/four"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_five"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/five"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_six"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/six"
                        android:background="#00000000" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/region_seven_to_nine"
                    android:orientation="horizontal"
                    android:gravity="center"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/button_seven"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/seven"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_eight"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/eight"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_nine"
                        android:layout_width="90dp"
                        android:layout_height="61dp"
                        android:src="@drawable/nine"
                        android:background="#00000000" />
                </LinearLayout>
                <LinearLayout
                    android:id="@+id/region_zero"
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">
                    <ImageButton
                        android:id="@+id/button_xin"
                        android:layout_width="89dp"
                        android:layout_height="61dp"
                        android:src="@drawable/button_xin"
                        android:background="#00000000" />

                    <ImageButton
                        android:id="@+id/button_zero"
                        android:layout_width="89dp"
                        android:layout_height="61dp"
                        android:src="@drawable/zero"
                        android:background="#00000000" />
                    <ImageButton
                        android:id="@+id/button_jin"
                        android:layout_width="89dp"
                        android:layout_height="61dp"
                        android:src="@drawable/button_jin"
                        android:background="#00000000" />
                </LinearLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">
                <ImageButton
                    android:layout_width="89dp"
                    android:layout_height="122dp"
                    android:gravity="center"
                    android:src="@drawable/button_send"
                    android:background="#00000000" />
                <ImageButton
                    android:layout_width="89dp"
                    android:layout_height="61dp"
                    android:src="@drawable/button_mic"
                    android:background="#00000000" />
                <ImageButton
                    android:layout_width="89dp"
                    android:layout_height="61dp"
                    android:src="@drawable/button_search"
                    android:background="#00000000" />
            </LinearLayout>
        </LinearLayout>
    </LinearLayout>
</com.gangle.RotateLayout>
